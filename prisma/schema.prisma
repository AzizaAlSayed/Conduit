generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  userid   String    @id @default(uuid())
  username String    
  articls  Article[]
}

model Article {
  articleid String @id @default(uuid())
  comment   String
  writer    User   @relation(fields: [writerid], references: [userid])
  writerid  String
}
